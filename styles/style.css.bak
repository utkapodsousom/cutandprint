.borderOrange p {
	width:60%;
	font-size: 1.4em;
}

	.borderOrange p:first-child {
		width: 60%;
	}

.borderOrange2 p {
	font-size: 1.4em;
	width:60%;
}

.boldOrange p {
	width:60%;
	font-size: 1.4em;
}

	.boldOrange p:last-child {
		font-size:2em;
	}

#paynget {
	height:600px;
}

.contactIcons {
	width: 420px;
}

	.contactIcons > h2 {
		color:#505050;
	}

	.contactIcons > h2:last-child {
		font-size: 1.8em;
	}

.map {
	width:60%;
}

	.map p {
		font-size: 1.4em;
	}

